// Place your settings in this file to overwrite the default settings
{
  "remote.SSH.remotePlatform": {
    "coder-vscode--shih--lyte--main": "linux"
  },
  "remote.SSH.connectTimeout": 1800,
  "workbench.colorTheme": "One Monokai",
  "workbench.iconTheme": "material-icon-theme",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "python.defaultInterpreterPath": "/Users/sam.shih/.nix-profile/bin/python3.11",
  "github.copilot.editor.enableAutoCompletions": true,
  "githubPullRequests.pullBranch": "never",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "lldb.library": "/Applications/Xcode-15.0.0.15A240d.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "is:open assignee:${user} repo:${owner}/${repository}",
      "groupBy": ["milestone"]
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Recent Issues",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "githubPullRequests.queries": [
    {
      "label": "Waiting For My Review",
      "query": "repo:${owner}/${repository} is:open review-requested:${user}"
    },
    {
      "label": "Reviewed by Me",
      "query": "repo:${owner}/${repository} is:open  reviewed-by:${user}"
    },
    {
      "label": "Assigned To Me",
      "query": "repo:${owner}/${repository} is:open assignee:${user}"
    },
    {
      "label": "Created By Me",
      "query": "repo:${owner}/${repository} is:open author:${user}"
    },
    {
      "label": "Mentioned Me",
      "query": "repo:${owner}/${repository} is:open mentions:${user}"
    }
  ],
  "editor.rulers": [100, 120],
  "editor.renderWhitespace": "all",
  "diffEditor.experimental.showMoves": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "explorer.excludeGitIgnore": true,
  "files.trimTrailingWhitespace": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.blame.editorDecoration.enabled": true,
  "[yaml]": {
    "diffEditor.ignoreTrimWhitespace": true
  },
  "window.confirmBeforeClose": "keyboardOnly",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "geminicodeassist.project": "upheld-setting-396705",
  "diffEditor.codeLens": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "redhat.telemetry.enabled": false,
  "window.zoomLevel": -1
}